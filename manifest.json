{
  "name": "Ferramenta Fusion DEV",
  "version": "1.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["*://*/fusion/*"],
      "js": ["script.js"]
    }
  ],
  "action": {
    "default_title": "Ferramenta Fusion DEV",
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab", "storage"]
}
